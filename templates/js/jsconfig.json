{
    "compilerOptions": {
        "module": "esnext",
        "target": "esnext",
        "checkJs": false,
        "noImplicitAny": false,
        "allowJs": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@titan": ["./app/titan"],
            "*": ["./app/*"]
        }
    },
    "include": [
        "app/**/*",
        "titan/**/*"
    ]
}